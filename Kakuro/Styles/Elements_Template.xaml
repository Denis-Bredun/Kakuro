<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate x:Key="Elements_Template">
        <TextBox Height="40" Width="40" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"
         Style="{DynamicResource CustomTextBoxStyle}"
         IsReadOnly="{Binding DataContext.IsGameCompleted, RelativeSource={RelativeSource AncestorType=UserControl}}"/>
    </DataTemplate>
</ResourceDictionary>
